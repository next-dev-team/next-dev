{
  "name": "next-dev",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev_": "dumi dev",
    "pre:dumi": "dumi preview",
    "build:dumi": "dumi build",
    "prepare": "dumi setup",
    "setup": "pnpm install",
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue",
    "test": "turbo run test",
    "doc:dev": "dumi dev",
    "doc:build": "pnpm clean &&  turbo run build --filter ./doc... && pnpm build:dumi",
    "doc:predeploy": "turbo run publish --filter ./boilerplate/doc...",
    "doc:deploy": "pnpm doc:build",
    "lint": "turbo run lint",
    "upd-main": "pnpm --interactive update --latest",
    "upd": "turbo run upd"
  },
  "authors": [],
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "dependencies": {
    "dumi": "^2.2.17",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-components": "^2.6.48",
    "antd": "^5.13.2",
    "@types/react": "^18.2.48",
    "typescript": "^5.3.3",
    "cross-env": "^7.0.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "turbo": "^1.11.3",
    "@ant-design/icons": "^5.2.6",
    "dumi-theme-antd-style": "^0.29.7",
    "babel-plugin-antd-style": "^1.0.4"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=18.0.0"
  }
}