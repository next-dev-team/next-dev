{
  "name": "@rnr/showcase",
  "main": "index.js",
  "version": "0.0.3",
  "scripts": {
    "start": "ENV=development expo start --clear --dev-client",
    "device:ios": "ENV=development expo run:ios --device",
    "device:ios:prod": "expo run:ios --device",
    "ios": "ENV=development expo run:ios",
    "ios:prod": "expo run:ios",
    "device:android": "ENV=development expo run:android --device",
    "device:android:prod": "expo run:android --device",
    "android": "ENV=development expo run:android",
    "android:prod": "expo run:android",
    "prebuild": "ENV=development expo prebuild --clean",
    "prebuild:ios": "ENV=development expo prebuild --platform ios --clean",
    "prebuild:android": "ENV=development expo prebuild --platform android --clean",
    "prebuild:prod": "expo prebuild --clean",
    "testflight": "eas build -p ios --submit",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production --auto-submit",
    "update:ios": "eas update --platform ios",
    "build:web": "npx expo export -p web",
    "serve:web": "npx serve dist --single",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@expo-google-fonts/geist": "^0.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/material-top-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/aspect-ratio": "~1.2.0",
    "@rn-primitives/collapsible": "~1.2.0",
    "@rn-primitives/label": "~1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/slider": "~1.2.0",
    "@rn-primitives/toast": "~1.2.0",
    "@rn-primitives/toolbar": "~1.2.0",
    "@rnr/registry": "workspace:*",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-table": "^8.11.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^54.0.12",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1302.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-mmkv": "^3.3.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "~0.21.1",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.17",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.9.3"
  },
  "private": true
}
