{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "app/*": ["./packages/app/*"],
      "@next-dev/rn-ui/*": ["./packages/rn-ui/*"],
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "extends": "./tsconfig.base.json",
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/todo-next/out",
    "apps/todo-next/.next",
    "apps/todo-next/.tamagui"
  ],
  "include": [".dumirc.ts"]
}
